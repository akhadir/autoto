{
  "manifest_version": 2,
  "name": "Autoto",
  "description": "Puppeteer Testcases Using Compare",
  "version": "0.1",
  "minimum_chrome_version": "10.0",
  "browser_action": {
    "default_icon": "icon.png",
    //"default_popup": "popup.html",
    "default_title": "Generate Testcases here!"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": [],
      "js": ["extJs/jquery.js", "extJs/html2canvas.js", "chromeJs/inject.js", "chromeJs/inject-new.js"],
      "run_at": "document_start"
    }
  ],
  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["chromeJs/bg-new.js"]
  },

  "permissions": [
    "debugger",
    "activeTab",
    "webRequest",
    "background",
    "tabs",
    "storage",
    "file://*/*",
    "https://*/",
    "https://*/*",
    "http://*/",
    "http://*/*",
    "<all_urls>"
  ]
}